<div id="index_02" class="row h-100 poor-story">
    <div class="row">
        <div class="col l4 offset-l4 m8 offset-m2 s12" style="margin-top:10vh;">
            <div class="card c5">
                <form class="card-content white-text" name="coupleCnl" action="coupleCnl" method="POST">
                    <div id="dvJoin">
                        <p><i class="medium material-icons">send</i></p>
                        <input type="hidden" id="couplePid" name="couplePid" value=<%- match %>>
                        <h5>유저번호 <%- match %>번님께 커플요청이 진행중입니다.<br/>취소하시겠습니까?</h5><br/><br/>
                        <div class="row">
                            <div class="col s12 center">
                                <button id="liCoupleCancelSubmit" class="btn waves-effect waves-light red poor-story" onclick="button_event()">
                                    <a href="#"></a>취소합니다<i class="material-icons left">close</i></a>
                                </button>
                            </div>      
                        </div>
                    </div>
                </form>
            </div>
        </div>
    
        <div class="col s12 m12 center-align">
        </div>
    </div>
</div>

<script type="text/javascript">
    // 동작이 안돼 ㅠㅠㅠㅠㅠㅠㅠ
    function button_event(){
        var result = confirm("정말 취소하시겠습니까??");
        if(!result){ return false; }
    }

    $(document).ready(function () {
        var error = <%- error %>;
        if(error == 1)
            alert("요청 신청이 완료되었습니다!");
    });
</script>
